                    $("#UsererrorCode").html("User already added.");

                }
            }
        }
        else {
            $("#searchmodal").modal('show');
            $("#UsererrorCode").css({ "color": "red" });
            $("#UsererrorCode").html(EmployeeName + ' is not approver');
            //alert("Initator cannot be nominated as approver");
        }
    }
</script>



