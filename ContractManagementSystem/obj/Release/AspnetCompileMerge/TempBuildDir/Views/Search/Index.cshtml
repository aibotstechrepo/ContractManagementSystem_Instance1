                    $.each(response, function (index, value) {
                        var option = document.createElement("option");
                        option.text = value;
                        option.setAttribute('Value', value);
                        x.add(option);
                    });
                },
                failure: function (response) {
                    alert(response);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        }
