    function SaveVendorTemplate() {
        var content = CKEDITOR.instances.IDofEditor.getData();
        content = content.replace(/\?/g, "");
        var Select = $("#type_of_entity").val();

        $.ajax({
            type: "POST",
            url: "/Vendor/SaveVendorTemplate",
            traditional: true,

            data: { content: content, Select: Select },
            success: function (response) {
                if (response == "success") {
                    swal("Saved Successfully", {
                        icon: "success",
                    }).then(function (result) {
                        if (result) {
                            window.location = '/Vendor/VendorTemplate';
                        }
                    });
                }
                else {
                    swal("Unable to Save Data", {
                        icon: "error",
                        text: "Unable to Save Data. Please try again",
                    }).then(function (error) {
                        if (error) {
                            window.location = '/Template/TemplateAmendments';
                        }
                    });
                }
            },

        });
    }

</script>
