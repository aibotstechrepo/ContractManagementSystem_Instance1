<script>
    function TypeOfEntity(Option) {
        if (Option == "Partnership Firm") {
            $(".IDforPartnership").show();
            $("#Vendor_Name").html("Name of the Partnership Firm");
            $("#IDforCompany").hide();
            $("#IDforSole").hide();
            $("#IDforLimited").hide();
            $("#IDforOthers").hide();
        }
        else if (Option == "Company") {
            $("#IDforCompany").show();
            $("#Vendor_Name").html("Name of the Company");
            $(".IDforPartnership").hide();
            $("#IDforSole").hide();
            $("#IDforLimited").hide();
            $("#IDforOthers").hide();
        }
        else if (Option == "Sole Proprietorship") {
            $(".IDforPartnership").hide();
            $("#IDforCompany").hide();
            $("#IDforSole").show();
            $("#Vendor_Name").html("Name of the Proprietorship Concern");
            $("#IDforLimited").hide();
            $("#IDforOthers").hide();
        }
        else if (Option == "Limited Liability Partnership Firm") {
            $(".IDforPartnership").hide();
            $("#IDforCompany").hide();
            $("#IDforSole").hide();
            $("#IDforLimited").show();
            $("#Vendor_Name").html("Name of the Limited Liability Partnership");
            $("#IDforOthers").hide();
        }
        else if (Option == "Others") {
            $(".IDforPartnership").hide();
            $("#IDforCompany").hide();
            $("#IDforSole").hide();
            $("#IDforLimited").hide();
            $("#IDforOthers").show();
            $("#Vendor_Name").html("Name");
        }
    }
</script>


