        var VendorRegisteredAddress = $('#Variable_RegisteredAddress').val();


        var VendorNPC = $('#Variable_Name_of_the_Proprietorship_Concern').val();
        var VendorNP = $('#Variable_Name_of_the_Proprietorship').val();
        var VendorPAN = $('#Variable_PAN_Number').val();
        var VendorAdhaar = $('#Variable_Adhaar_Card_No').val();
        var VendorAddressP = $('#Variable_Address_of_the_Proprietorship_Concern').val();


        var VendorLLPF = $('#Variable_Limited_Liability_Partnership_Firm').val();
        var VendorLLPIN = $('#Variable_LLP_IN').val();
        var VendorAddressLLPIN = $('#Variable_Registered_Address_of_the_LLP').val();


        var VendorOtherName = $('#Variable_Name_Others').val();
        var VendorOtherAddress = $('#Variable_Address').val();


        var VendorNamePF = $('#Variable_Name_of_the_Partnership_Firm').val();
        //var VendorAuthSignPF = $('#Variable_Authorised_Signatory_Partnership').val();
        //var VendorAddressPF = $('#Variable_Registered_Address_Partnership').val();
        var VendorName1PF = $('#Vendor_Name_of_1st_Partner').val();
        var VendorAge1PF = $('#Vendor_Age_of_1st_Partner').val();
        var VendorHusband1PF = $('#Vendor_Father_Husband_Name_of_1st_Partner').val();
        var VendorAddress1Partner = $('#Vendor_Address_of_1st_Partner').val();


        var Allowed = [];


        if (content == '') {
            Allowed.push('Content');
        }

        if (DurationFrom.length < 1) {
            Allowed.push('Duration From');
        }
        if (DurationTo.length < 1) {
            Allowed.push('Duration To');
        }
        if (Entity == "Company") {
            if (VendorName.length < 1) {
                Allowed.push('Name of the Company');
            }
            if (VendorCin.length < 1) {
                Allowed.push('CIN');
            }
            if (VendorSignatory.length < 1) {
                Allowed.push('Authorised Signatory');
            }
            if (VendorCorporateAddress.length < 1) {
                Allowed.push('Corporate Address');
            }
            if (VendorRegisteredAddress.length < 1) {
                Allowed.push('Registered Address');
            }
        }

        if (Entity == "Sole Proprietorship") {
            if (VendorNPC.length < 1) {
                Allowed.push('Name of the Proprietorship Concern');
            }
            if (VendorNP.length < 1) {
                Allowed.push('Name of the Proprietorship');
            }
            if (VendorPAN.length < 1) {
                Allowed.push('PAN Number');
            }
            if (VendorAdhaar.length < 1) {
                Allowed.push('Adhaar Card No');
            }
            if (VendorAddressP.length < 1) {
                Allowed.push('Address of the Proprietorship Concern');
            }
        }


        if (Entity == "Limited Liability Partnership Firm") {
            if (VendorLLPF.length < 1) {
                Allowed.push('Limited Liability Partnership Firm');
            }
            if (VendorLLPIN.length < 1) {
                Allowed.push('LLP IN');
            }
            if (VendorAddressLLPIN.length < 1) {
                Allowed.push('Variable_Registered_Address_of_the_LLP');
            }
        }

        if (Entity == "Others") {
            if (VendorOtherName.length < 1) {
                Allowed.push('Name');
            }
            if (VendorOtherAddress.length < 1) {
                Allowed.push('Address');
            }
        }

        if (Entity == "Partnership Firm") {
            if (VendorNamePF.length < 1) {
                Allowed.push('Name of the Partnership Firm');
            }
            if (VendorSignatory.length < 1) {
                Allowed.push('Authorised Signatory');
            }
            if (VendorRegisteredAddress.length < 1) {
                Allowed.push('Registered Address');
            }
            if (VendorName1PF.length < 1) {
                Allowed.push('Name of 1st Partner');
            }
            if (VendorAge1PF.length < 1) {
                Allowed.push('Age of 1st Partner');
            }
            if (VendorHusband1PF.length < 1) {
                Allowed.push("Father's/Husband's Name of 1st Partner");
            }
            if (VendorAddress1Partner.length < 1) {
                Allowed.push('Address of 1st Partner');
            }
        }

        if (Allowed.length < 1) {
            $("InitiateModal").show();
        }

        else {
            document.getElementById("Initiate").disabled = true;
            //alert(" The word is Empty Cant be Initiated");
            swal({
                title: 'Contract Initiation',
                text: "The " + Allowed.join(', ') + " fields are required",
                icon: "warning",
            }).then(function (value) {
                if (value == true) {
                    document.getElementById("Initiate").disabled = false;
                }
            })

        }
    }

</script>

